{
  "name": "GitHub Glitch button",
  "description": "Import from a public GitHub repo into Glitch",
  "version": "1.1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  },
  "author": "Zac Colley",
  "homepage_url": "https://github.com/zaccolley/github-glitch-button",
  "content_scripts": [
  {
    "matches": ["*://github.com/*"],
    "css": ["styles.css"],
    "js": ["scripts.js"]
  }
  ],
  "permissions": [
  "tabs", "*://github.com/*"
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "42"
}
